00183     {\cf19 for} ({\cf18 int} coordY = 0; coordY < columns; ++coordY)\par
00184     \{\par
00185       {\cf19 for} ({\cf18 int} iterador = 0; iterador < 10; ++iterador)\par
00186       \{\par
00187         {\cf19 if} (matrix[coordX][coordY] == {\cf23 '+'} &&\par
