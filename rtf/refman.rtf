00192         \}\par
00193       \}\par
00194     \}\par
00195   \}\par
00196 \par
00197   {\cf19 if} (faltou != 0)\par
00198   \{\par
00199     {\cf19 return} 0;\par
00200   \}\par
00201   {\cf19 else} {\cf19 if} (faltou == 0)\par
00202   \{\par
00203     {\cf19 return} 1;\par
00204   \}\par
